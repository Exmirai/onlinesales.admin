{
  "name": "onlinesales.admin",
  "version": "0.0.1",
  "private": true,
  "description": "React based Admin UI for OnlineSales (lightweight, extendable headless CMS for product websites)",
  "keywords": [
    "onlinesales"
  ],
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" webpack serve --config=\"./webpack/webpack.config.ts\"  --mode=development",
    "build": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" webpack --config=\"./webpack/webpack.config.ts\" --mode=production",
    "lint": "eslint ./src/**/*",
    "format": "prettier --write ./src/**/*",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterliapin/onlinesales.admin.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peterliapin/onlinesales.admin/issues"
  },
  "homepage": "https://github.com/peterliapin/onlinesales.admin#readme",
  "engines": {
    "node": "^18.13.0",
    "npm": "^9.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
